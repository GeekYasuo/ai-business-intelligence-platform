{
  "name": "ai-business-intelligence-backend",
  "version": "2.0.0",
  "description": "Enterprise AI-powered business intelligence backend with advanced analytics and ML integration",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "nodemon src/server.ts",
    "build": "tsc && tsc-alias",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --config jest.integration.config.js",
    "test:e2e": "jest --config jest.e2e.config.js",
    "lint": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "docker:build": "docker build -t ai-platform-backend .",
    "docker:run": "docker run -p 3001:3001 ai-platform-backend",
    "deploy:aws": "serverless deploy --stage production",
    "deploy:gcp": "gcloud app deploy",
    "benchmark": "node dist/benchmark/performance.js",
    "docs": "typedoc src --out docs",
    "security:audit": "npm audit && snyk test"
  },
  "keywords": [
    "artificial-intelligence",
    "business-intelligence",
    "analytics",
    "machine-learning",
    "typescript",
    "nodejs",
    "enterprise",
    "real-time",
    "api",
    "graphql"
  ],
  "author": {
    "name": "Himanshu Singh",
    "email": "himanshu@example.com",
    "url": "https://github.com/GeekYasuo"
  },
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "typescript": "^5.3.3",
    "@types/node": "^20.11.0",
    "@types/express": "^4.17.21",
    "apollo-server-express": "^3.12.1",
    "graphql": "^16.8.1",
    "graphql-tools": "^9.0.0",
    "socket.io": "^4.7.5",
    "@types/socket.io": "^3.0.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "@types/bcryptjs": "^2.4.6",
    "jsonwebtoken": "^9.0.2",
    "@types/jsonwebtoken": "^9.0.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-google-oauth20": "^2.0.0",
    "joi": "^17.12.0",
    "pg": "^8.11.3",
    "@types/pg": "^8.10.9",
    "knex": "^3.1.0",
    "redis": "^4.6.13",
    "ioredis": "^5.3.2",
    "bull": "^4.12.2",
    "@types/bull": "^4.10.0",
    "axios": "^1.6.7",
    "openai": "^4.28.0",
    "nodemailer": "^6.9.9",
    "@types/nodemailer": "^6.4.14",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "morgan": "^1.10.0",
    "@types/morgan": "^1.9.9",
    "multer": "^1.4.5",
    "@types/multer": "^1.4.11",
    "sharp": "^0.33.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "uuid": "^9.0.1",
    "@types/uuid": "^9.0.7",
    "lodash": "^4.17.21",
    "@types/lodash": "^4.14.202",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "@types/swagger-ui-express": "^4.1.6",
    "cron": "^3.1.6",
    "csv-parser": "^3.0.0",
    "csv-stringify": "^6.4.5",
    "fast-csv": "^5.0.1",
    "jsonwebtoken": "^9.0.2",
    "node-cache": "^5.1.2",
    "prometheus-client": "^15.1.0",
    "elastic-apm-node": "^4.0.0",
    "newrelic": "^11.7.0",
    "datadog-tracer": "^1.2.0",
    "opossum": "^8.0.0",
    "retry": "^0.13.1",
    "fast-json-stringify": "^5.12.0",
    "ajv": "^8.12.0",
    "class-validator": "^0.14.1",
    "class-transformer": "^0.5.1",
    "reflect-metadata": "^0.2.1",
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/graphql": "^12.0.11",
    "@nestjs/apollo": "^12.0.11",
    "@nestjs/typeorm": "^10.0.1",
    "@nestjs/bull": "^10.0.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/throttler": "^5.1.1",
    "@nestjs/schedule": "^4.0.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "supertest": "^6.3.4",
    "@types/supertest": "^6.0.2",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "prettier": "^3.2.4",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "typedoc": "^0.25.7",
    "cross-env": "^7.0.3",
    "rimraf": "^5.0.5",
    "concurrently": "^8.2.2",
    "wait-on": "^7.2.0",
    "snyk": "^1.1266.0",
    "npm-audit-resolver": "^3.0.0",
    "artillery": "^2.0.4",
    "autocannon": "^7.15.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GeekYasuo/ai-business-intelligence-platform.git"
  },
  "bugs": {
    "url": "https://github.com/GeekYasuo/ai-business-intelligence-platform/issues"
  },
  "homepage": "https://github.com/GeekYasuo/ai-business-intelligence-platform#readme",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": ["<rootDir>/src", "<rootDir>/tests"],
    "testMatch": ["**/__tests__/**/*.ts", "**/?(*.)+(spec|test).ts"],
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/types/**/*"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts,js}": ["eslint --fix", "prettier --write"],
    "*.{json,md}": ["prettier --write"]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}